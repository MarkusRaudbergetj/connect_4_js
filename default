server {
	listen 80 default_server;
	listen [::]:80 default_server;

	server_name test.domain.com;

	location / {
		proxy_pass http://localhost:3000;
		try_files $uri $uri/ =404;
	}
	location /styles.css {
		proxy_pass http://localhost:3000/styles.css;
	}
	location /index.js {
		proxy_pass http://localhost:3000/index.js;
	}
	location /read {
		proxy_pass http://localhost:3000/read;
	}
	location /write {
		proxy_pass http://localhost:3000/write;
	}
	location /player {
		proxy_pass http://localhost:3000/player;
	}
	location /setPlayer {
		proxy_pass http://localhost:3000/setPlayer;
	}
	location /images/empty_tile.jpg {
		proxy_pass http://localhost:3000/images/empty_tile.jpg;
	}
	location /images/downArror.jpg {
		proxy_pass http://localhost:3000/images/downArror.jpg;
	}
	location /images/yellow_tile.jpg {
		proxy_pass http://localhost:3000/images/yellow_tile.jpg;
	}
	location /images/red_tile.jpg {
		proxy_pass http://localhost:3000/images/red_tile.jpg;
	}
#	location /images/empty_tile.jpg {
#		proxy_pass http://localhost:3000/images/empty_tile.jpg;
#	}

}


